{
  "name": "SIL-FRONTEND_ASSESSMENT",
  "version": "1.0.0",
  "repository": "https://github.com/NoelLincoln/SIL-FRONTEND_ASSESSMENT.git",
  "author": "Noel Bryant <noelsobryant@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "install:all": "yarn install --cwd backend && yarn install --cwd client",
    "dev": "concurrently \"yarn --cwd backend dev\" \"yarn --cwd client dev\"",
    "build": "yarn --cwd backend build && yarn --cwd client build",
    "start": "concurrently \"yarn --cwd backend start\" \"yarn --cwd client preview\"",
    "test": "concurrently \"yarn --cwd backend test\" \"yarn --cwd client test\"",
    "lint:fix": "yarn --cwd backend lint:fix && yarn --cwd client lint:fix"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
